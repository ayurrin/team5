<% if user_signed_in? %>
  <p><%= link_to "マイページへ", user_path(current_user) %>
    <%= link_to("トップへ", "/home/top") %>
  </p>
  <h2>ユーザー一覧画面</h2>
  <% User.all.each do |user| %>
    <p>
      <%= "id: #{user.id} email: #{user.email}" %>
      <% unless user == current_user %>
        <%= link_to "詳細", user_path(user) %>
      <% end %>
    </p>
  <% end %>
<% end %>
